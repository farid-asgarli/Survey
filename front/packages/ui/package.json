{
  "name": "@survey/ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rimraf dist .turbo tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No lint configured for ui package'"
  },
  "dependencies": {
    "@survey/types": "workspace:*",
    "@survey/ui-primitives": "workspace:*",
    "lucide-react": "^0.556.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3"
  }
}
